{
  "name": "salesforce-log-ai-analyzer-mcp",
  "version": "0.8.0",
  "description": "AI-First Salesforce Debug Log Analyzer - Local-first, AI-optimized debug log analysis for Salesforce via MCP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalesforceDiariesBySanket/salesforce-log-ai-analyzer-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/SalesforceDiariesBySanket/salesforce-log-ai-analyzer-mcp/issues"
  },
  "homepage": "https://github.com/SalesforceDiariesBySanket/salesforce-log-ai-analyzer-mcp#readme",
  "bin": {
    "sf-debug": "./dist/cli/index.js",
    "sf-debug-mcp": "./dist/mcp/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:parser": "vitest src/parser",
    "test:analyzer": "vitest src/analyzer",
    "test:detectors": "vitest src/analyzer/detectors",
    "test:managed": "vitest src/managed",
    "test:output": "vitest src/output",
    "test:levels": "vitest src/analyzer/level-*",
    "test:privacy": "vitest src/privacy",
    "test:async": "vitest src/async",
    "test:mcp": "vitest src/mcp",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "mcp": "node dist/mcp/index.js"
  },
  "keywords": [
    "salesforce",
    "debug-log",
    "analyzer",
    "mcp",
    "ai",
    "apex",
    "soql",
    "model-context-protocol",
    "debugging",
    "log-parser",
    "github-copilot",
    "claude"
  ],
  "author": "SalesforceDiariesBySanket",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.6.2",
    "commander": "^11.1.0",
    "zod": "^3.25.76"
  }
}
